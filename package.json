    "sass": "1.62.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.8.3",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.15.3",
    "vite": "3.1.3",
    "ws": "8.13.0"
  }
